                 <script>
                     window.onscroll = function () {
                         var body = document.body;
                         var html = document.documentElement;
                         var scrollTop = window.scrollY || document.documentElement.scrollTop;
                         var offsetHeight = Math.max(body.scrollHeight, body.offsetHeight, html.clientHeight, html.scrollHeight, html.offsetHeight);
         
                         console.log(offsetHeight)
                         console.log(scrollTop)
         
                         if (scrollTop > 150) {
                             body.style.backgroundImage = "url('images/bgwithouttext.svg')"; // Put the new image URL here
                             body.style.backgroundSize = "cover";
                             body.style.backgroundRepeat = "no-repeat";
                             body.style.backgroundPosition = "center";
                             body.style.backgroundAttachment = "fixed";
                             body.style.transition = "0.1s ease-in"
                         } else {
                             // Reset to the original background image or remove these lines if not needed
                             body.style.backgroundImage = "";
                             body.style.backgroundSize = "";
                             body.style.backgroundRepeat = "";
                             body.style.backgroundPosition = "";
                             body.style.backgroundAttachment = "";
                         }
                     };
                 </script>
         
